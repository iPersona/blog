{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [{
            "type": "node",
            "request": "launch",
            "name": "Debug script",
            "program": "${workspaceFolder}/docker/scripts/index.js"
        },
        {
            "name": "Python: Current file",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'blog'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=blog"
                ],
                "filter": {
                    "name": "blog",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'blog'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=blog",
                    "--package=blog"
                ],
                "filter": {
                    "name": "blog",
                    "kind": "bin"
                }
            },
            "args": [
                "-w", "${workspaceFolder}"
            ],
            "env": {
                "RUST_BACKTRACE": "full"
            },
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'blog'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=blog",
                    "--package=blog"
                ],
                "filter": {
                    "name": "blog",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "vuejs: chrome",
            "url": "http://localhost:8880",
            "webRoot": "${workspaceFolder}/client/src",
            "breakOnLoad": true,
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "${webRoot}/*"
            }
        },
        {
            "type": "firefox",
            "request": "launch",
            "name": "vuejs: firefox",
            "url": "http://localhost:8880",
            "webRoot": "${workspaceFolder}/client/src",
            "pathMappings": [{
                "url": "webpack:///src/",
                "path": "${webRoot}/"
            }]
        }
    ]
}